@model SFA.DAS.GovUK.Auth.Models.StubAuthUserDetails

<div class="govuk-grid-row" id="estimate-start-transfer">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-xl">You've signed in locally</h1>
        <p>Id: @Model.Id</p>
        <p>Id: @Model.Email</p>

        <ul>
            @foreach (var claim in User.Claims)
            {
                <li><strong>@claim.Type:</strong> @claim.Value</li>
            }
        </ul>

        <a asp-action="SigningOut" asp-controller="Account" class="govuk-button">Sign Out</a>

    </div>
</div>