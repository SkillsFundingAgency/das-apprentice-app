<!DOCTYPE html>
<html lang="en" class="govuk-template app-template">

<head>
    <meta charset="utf-8" />
    <meta name="viewport"
        content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, viewport-fit=cover" />
    <meta name="theme-color" content="#0b0c0c" />
    <title>@ViewData["Title"] - Apprentice App</title>
    <link rel="icon" sizes="48x48" href="/assets/images/favicon.ico" />
    <link rel="apple-touch-icon" href="/assets/images/apple-touch-icon.png" />
    <link rel="stylesheet" href="~/css/app.css" asp-append-version="true" />
</head>

<body class="govuk-template__body app-template__body" id="top">
    <main class="app-main--basic">
        @RenderBody()
    </main>
    @await Html.PartialAsync("_FrontendJs.cshtml")
</body>

</html>